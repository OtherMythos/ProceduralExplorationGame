
compositor_node renderTextureNode
{
    in 0 renderTexture
    target renderTexture
    {
        pass clear
        {
            colour_value 0.2 0.2 0.2 1
        }
        pass render_quad
        {
            material worldGenToolViewer
        }
    }
}

workspace renderTextureWorkspace
{
    connect_output renderTextureNode 0
}