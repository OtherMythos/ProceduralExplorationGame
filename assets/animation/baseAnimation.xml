                <k t='185' scale='2, 2, 2'/>
                <k t='190' scale='1, 1, 1'/>
                <k t='195' scale='1, 1, 1'/>
                <k t='200' scale='2, 2, 2'/>
                <k t='205' scale='1, 1, 1'/>

                <k t='240' scale='1, 1, 1'/>
            </t>
        </HealthOrbAnim>
        <PlaceBeaconIdle repeat='true' end='1000'>
            <t type='transform' target='0'>
                <k t='0' position='0, 1, 0'/>
                <k t='500' position='0, 1.5, 0'/>
                <k t='1000' position='0, 1, 0'/>
            </t>
        </PlaceBeaconIdle>
    </animations>
</AnimationSequence>
