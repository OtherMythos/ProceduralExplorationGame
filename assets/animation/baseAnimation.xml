            <t type='transform' target='1'>
                <k t='0' scale='1, 1, 1'/>

                <k t='60' scale='1, 1, 1'/>
                <k t='65' scale='2, 2, 2'/>
                <k t='70' scale='1, 1, 1'/>
                <k t='75' scale='1, 1, 1'/>
                <k t='80' scale='2, 2, 2'/>
                <k t='85' scale='1, 1, 1'/>


