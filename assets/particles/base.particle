particle_system playerWalkingDust
{
    material        ParticleAlphaGrey
    particle_width  0.2
    particle_height 0.2
    cull_each       true
    quota           5000
    billboard_type  point

    // Box emitter for ambient dust.
    emitter Box
    {
        angle           0
        emission_rate   20
        time_to_live    5
        direction       0 1 0
        angle           0
        velocity        0.1
        width           200
        height          200
        depth           16
    }

    affector ColourInterpolator
    {
        time0 0
        colour0 1 1 1 0

        time1 0.1
        colour1 1 1 1 1

        time2 1.0
        colour2 1 1 1 0
    }

    affector DirectionRandomiser
    {
        randomness 0.1
    }


}

particle_system playerFeetDust
{
    material        ParticleAlphaWhite
    particle_width  0.3
    particle_height 0.3
    cull_each       false
    quota           2000
    billboard_type  point

    //Ring emitter at player feet projecting outward
    emitter GroundColourModifier
    {
        angle           25
        emission_rate   5
        time_to_live    0.8
        direction       0 1 0
        velocity        10.0
        inner_width     0.8
        inner_height    0.8
        width           2.5
        height          2.5
        depth           0.01
    }

    affector DirectionRandomiser
    {
        randomness 0.1
    }

    affector Scaler
    {
        rate -0.16
    }

    // Gravity.
    affector LinearForce
    {
        force_vector      0 -14 0
        force_application add
    }

	affector DeflectorPlaneProceduralWorld
	{
		plane_point	0 1 0
		plane_normal	0 1 0
		bounce	0.5
	}
}

