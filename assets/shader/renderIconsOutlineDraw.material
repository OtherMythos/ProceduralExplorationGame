
fragment_program Postprocess/renderIconsOutlineDraw_ps_GLSL glsl
{
	source renderIconsOutlineDraw_ps.glsl
	default_params
	{
		param_named Image int 0
	}
}

fragment_program Postprocess/renderIconsOutlineDraw_ps_VK glslvk
{
	source renderIconsOutlineDraw_ps.glsl
}

fragment_program Postprocess/renderIconsOutlineDraw_ps_Metal metal
{
	source renderIconsOutlineDraw_ps.metal
	shader_reflection_pair_hint Ogre/Compositor/Quad_vs
}

fragment_program Postprocess/renderIconsOutlineDraw_ps unified
{
	delegate Postprocess/renderIconsOutlineDraw_ps_GLSL
	delegate Postprocess/renderIconsOutlineDraw_ps_VK
	delegate Postprocess/renderIconsOutlineDraw_ps_Metal
}

material Postprocess/RenderIconsOutlineDraw
{
    technique
    {

		pass
		{
			depth_check off
			depth_write off

			cull_hardware none

			vertex_program_ref Ogre/Compositor/Quad_vs
			{
			}

			fragment_program_ref Postprocess/renderIconsOutlineDraw_ps
			{
			}

			texture_unit Image
			{
				tex_address_mode clamp
				filtering trilinear
			}
		}
	}
}
