
compositor_node mapViewer/renderTextureNode
{
    in 0 renderTexture
    target renderTexture
    {
        pass clear
        {
            colour_value 0.2 0.2 0.2 1
        }
        pass render_quad
        {
            material mapViewer/mapMaterial
        }
    }
}

workspace mapViewer/renderTextureWorkspace
{
    connect_output mapViewer/renderTextureNode 0
}